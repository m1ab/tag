<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <groupId>ru.lumo.html</groupId>
    <artifactId>tag-parent</artifactId>
    <packaging>pom</packaging>
    <version>1.0-SNAPSHOT</version>


    <name>Tag Libraries</name>
    <url>http://tag.lumo.ru</url>

    <profiles>
        <profile>
            <id>total</id>
            <modules>
                <module>tag-core</module>
                <module>tag-bs</module>
            </modules>
        </profile>
    </profiles>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>3.5.1</version>
                <configuration>
                    <source>1.8</source>
                    <target>1.8</target>
                </configuration>
            </plugin>
        </plugins>
    </build>
    <distributionManagement>
        <repository>
            <id>lumo-releases</id>
            <url>http://192.168.5.138:8081/nexus/content/repositories/lumo-releases</url>
        </repository>
        <snapshotRepository>
            <id>lumo-snapshots</id>
            <url>http://192.168.5.138:8081/nexus/content/repositories/lumo-snapshots</url>
        </snapshotRepository>
        <!--<snapshotRepository>-->
            <!--<id>common-snapshots</id>-->
            <!--<url>http://team/nexus/content/repositories/common-snapshots</url>-->
        <!--</snapshotRepository>-->
    </distributionManagement>

</project>
